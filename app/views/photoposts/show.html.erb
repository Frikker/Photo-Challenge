<div class="col-md-5">
  <h4><%= @photopost.user.first_name %> <%= @photopost.user.last_name %></h4>
  <h6><%= time_ago_in_words(@photopost.created_at) %> ago</h6>
  <%= @photopost.content %>
  <%= @photopost.picture %>
</div>
<% if @comments.any? %>
  <%= will_paginate @comments %>
  <ul>
    <%= render @comments %>
  </ul>
  <%= will_paginate @comments %>
<% end %>
<%= render 'comments/comment_form' %>